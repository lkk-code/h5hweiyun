.Text{
    width: px2vw(409);
    height: px2vw(468);
    color: #fff;
    font-size:px2vw(24);
    position: relative;
    left: px2vw(73);
    letter-spacing:px2vw(1.5);
}
.data_box .polygon img:nth-child(1){
    width: px2vw(95);
    height: px2vw(76);
    position: absolute;
    left: px2vw(420);
    top: px2vw(547);
}
.data_box .polygon img:nth-child(2){
    width: px2vw(63);
    height: px2vw(59);
    position: absolute;
    left: px2vw(457);
    top: px2vw(556);
}
.data_box .btn_box{
    position: relative;
    top: px2vw(160);
    margin: auto;
    width: px2vw(293);
    height: px2vw(73);
    color: #fff;
    text-align: center;
    border: px2vw(2.5) solid #c8bfc5;
    background: rgba(1, 0, 55, 0.6);
    box-shadow: 0px 0px px2vw(10) #365586;
}
.btn_box p{
    line-height: px2vw(60);
}
.btn_box img{
    width: px2vw(75);
    height: px2vw(105);
    position: absolute;
    top: px2vw(-80);
    left: px2vw(146);
}
.foter_box{
    width: px2vw(755);
    height: px2vw(120);
    position: fixed;
    background-color: #150916;
    bottom: 0;
    padding: px2vw(20);
    padding-left: px2vw(30);
}
.InputText{
    width: px2vw(546);
    height: px2vw(80);
    line-height: 10px;
    background-color: #493f47;
    color: #8e8d8e;
    padding-left: px2vw(10);
    outline-color: invert ;
    outline-style: none ;
    outline-width: 0px ;
    border: none ;
    border-style: none ;
    text-shadow: none ;
    -webkit-appearance: none ;
    -webkit-user-select: text ;
    outline-color: transparent ;
    box-shadow: none;
}
.send{
    float: right;
    color: #e5e5e5;
    font-size: px2vw(32);
    position: relative;
    line-height: px2vw(80);
    right: px2vw(40);
}




