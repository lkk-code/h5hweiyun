/*顶部音乐按钮*/
.top_right_music_box{
    position: fixed;
    right: 0.16rem;
    top: 0.16rem;
    width: 0.64rem;
    height: 0.64rem;
    background-image: url("../img/common/music_start.png");
    background-size: 100% 100%;
    z-index: 999;
}
.top_right_music_box img{
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    margin: auto;
}
.top_right_music_box .music_start{
    width: 75%;
    height: 75%;
}
.top_right_music_box .music_stop{
    width: 100%;
    height: 100%;
    display: none;
}

.play_music{
    animation: music_disc 4s linear infinite;
    -webkit-animation: music_disc 4s linear infinite;
    -o-animation: music_disc 4s linear infinite;
}
@keyframes music_disc {
    0%{transform: rotate(0deg);-webkit-transform: rotate(0deg);}
    100%{transform: rotate(360deg);-webkit-transform: rotate(360deg);}}
@-webkit-keyframes music_disc {
    0%{transform: rotate(0deg);-webkit-transform: rotate(0deg);}
    100%{transform: rotate(360deg);-webkit-transform: rotate(360deg);}}