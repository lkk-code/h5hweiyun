
.bkg{
    height:100%;
    width: 100%;
    position: absolute;
    top: 0;
    left:0;
    z-index: -1!important;
}


@keyframes  startTwinkle{
    from{transform: scale(.8);opacity: 0.5}
    to{transform: scale(1);opacity: 1}
}



.data_box, .data_box2, .data_box3, .data_box4, .data_box5{
    background: none;
}
.data_box .bkg, .data_box5 .bkg{
    background: url("../img/image/bkg_red.png");
}
.data_box .bkg .powder:nth-child(1),
.data_box5 .bkg .powder:nth-child(1){
    width: 100%;
    height:70%;
    background: url("../img/image/powder1.png") no-repeat;
    background-size: 100%;
    position: absolute;
    top: 25%;
    left: 0;
}


.data_box2 .bkg, .data_box3 .bkg, .data_box4 .bkg{
    background: url("../img/image/bkg_blue.png");
}
.data_box2 .bkg .powder:nth-child(1),
.data_box3 .bkg .powder:nth-child(1),
.data_box4 .bkg .powder:nth-child(1){
    width: 100%;
    height:60%;
    background: url("../img/image/powder2.png") no-repeat;
    background-size: 100%;
    position: absolute;
    top: -10%;
    left: 50%;
}
.data_box2 .bkg .powder:nth-child(2),
.data_box3 .bkg .powder:nth-child(2),
.data_box4 .bkg .powder:nth-child(2){
    width: 100%;
    height:60%;
    background: url("../img/image/powder3.png") no-repeat;
    background-size: 100%;
    position: absolute;
    top: 70%;
    left: -40%;
}


.data_box3 .bkg{
    background: url("../img/image/bkg_blue.png");
}
