html,body{
	height: 100%;
}
.Box{
	 display: none;
 }
img{
	pointer-events: none!important;
}
textarea:focus, textarea:active, textarea, input
{
	border: none;
	outline: none;
	resize:none;

}
.data_box , .data_box2 , .data_box3 , .data_box4 , .data_box5{
	overflow: hidden;
	min-height: 12rem;
}
.data_box{
	position: relative;
	width: 100%;
	height: 100%;
	background: url("../img/image/LAIKA.jpg") no-repeat;
	max-width: 10rem;
	margin: auto;
	background-size: cover;
}

 .animation{
	width: 9.33rem;
	height: 13.65rem;
	position: absolute;
	top: 1.2rem;
	left: 0.7rem;
	background: url("../img/image/yuandian1.png") no-repeat;
	background-size: cover;
 animation-iteration-count: infinite;
 animation-direction: alternate;
 animation-timing-function: ease-in-out;
}
.animation{
	-webkit-animation:pulse 3s .2s ease-in-out infinite;
	-moz-animation:pulse 3s .2s ease-in-out infinite;
}
.animation1 img{
	-webkit-animation:pulse 3s .2s ease-in-out infinite;
	-moz-animation:pulse 3s .2s ease-in-out infinite;
}
.animation4 img{
	-webkit-animation:pulse 3s .2s ease-in-out infinite;
	-moz-animation:pulse 3s .2s ease-in-out infinite;
}

@-webkit-keyframes pulse{
	0%{-webkit-transform:scale(1)}
	50%{-webkit-transform:scale(1.1)}
	100%{-webkit-transform:scale(1)}
}
@-moz-keyframes pulse{
	0%{-moz-transform:scale(1)}
	50%{-moz-transform:scale(1.1)}
	100%{-moz-transform:scale(1)}
}
.hw_Img{
	padding-top: .54rem;
	padding-left: .42rem;
}
.hw_Img img{
	width: 2.72rem;
	height: .61rem;
}
.xy_img{
	margin-top: .37rem;
	margin-left: 0.42rem;
}
.xy_img img{
	width: 5.29rem;
	height: 1.22rem;
}
.data_box .frame{
	background: url("../img/image/juxing1.png")no-repeat;
	background-size: cover;
	width: 6.84rem;
	height: 7.84rem;
	position: relative;
	top: 3%;
	/*left: 1.6rem;*/
	margin: 0 auto;
}
.aircraft{
	position: relative;
	top: 0%;
	/*left: 1.38rem;*/
	margin: 0 auto;
}
.aircraft .air{
	position: absolute;
	width: 1.36rem;
	height: 0.91rem;
	animation: myfirst 1.5s;
	animation-timing-function: ease-out ;
}
@keyframes myfirst {
	from  {left:-5rem; top:5rem;transform:rotate(15deg);}
	to{ left:0; top:0;transform:rotate(0deg);}
}
.sjx{
	animation: myadiv .8s;
	animation-iteration-count: infinite;
	animation-direction: alternate;
	animation-timing-function: ease-in-out;
}
@keyframes myadiv {
	from   {transform:rotate(35deg);}
	to  {transform:rotate(0deg);}

}

.pen{
	animation: myaPen .8s;
	animation-iteration-count: infinite;
	animation-direction: alternate;
	animation-timing-function: ease-in-out;
}
@keyframes myaPen {
	from   { left:0;transform:rotateY(35deg);}
	to  {left:2rem;transform:rotateZ(0deg);}

}
