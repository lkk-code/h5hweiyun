html,body{
	height: 100%;
}
.data_box{
	position: relative;
	width: 100%;
	height: 100%;
	background: url("../img/image/beijing1.png") no-repeat;
	max-width: 10rem;
	margin: auto;
	background-size: cover;
}

.animation img{
	width: 9.43rem;
	height: 13.1rem;
	position: absolute;
	top: 2.4rem;

}
.hw_Img{
	padding-top: .54rem;
	padding-left: .42rem;
}
.hw_Img img{
	width: 2.72rem;
	height: .61rem;
}
.xy_img{
	margin-top: .57rem;
	margin-left: 0.42rem;
}
.xy_img img{
	width: 6.29rem;
	height: 1.52rem;
}
.data_box .frame{
	background: url("../img/image/juxing1.png")no-repeat;
	background-size: cover;
	width: 6.84rem;
	height: 7.84rem;
	position: relative;
	top: .9rem;
	left: 1.6rem;
}
.aircraft{
	position: absolute;
	top: 4rem;
	left: 1.38rem;
}
.aircraft img{
	width: 1.36rem;
	height: 0.91rem;
}

