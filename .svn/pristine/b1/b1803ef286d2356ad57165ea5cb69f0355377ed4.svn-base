@charset "UTF-8";
/*CSS Reset*/
body, div, ul, ol, li, dl, dt, dd, p, h1, h2, h3, h4, h5, h6, form, table, tr, td, th, button, input, select, textarea {
  margin: 0;
  padding: 0;
}

img {
  display: block;
  border: none;
  vertical-align: middle;
}

ol, ul {
  list-style: none;
}

button, input, select, textarea {
  font-size: 100%;
  outline: none;
}

input, select, textarea {
  -webkit-transition: all 0.30s ease-in-out;
  -moz-transition: all 0.30s ease-in-out;
  -ms-transition: all 0.30s ease-in-out;
  -o-transition: all 0.30s ease-in-out;
  border: 1px solid #ddd;
  font: 14px Arial,'Microsoft YaHei';

}
input:focus, select:focus, textarea:focus {
  border: 1px solid #5581f8!important;

}

/*textarea{resize:none;}*/
fieldset {
  border: 1px solid #c0c0c0;
  margin: 0 2px;
  padding: 0.35em 0.625em 0.75em;
}

/*em, i, strong, b {
  font-style: normal;
  font-weight: normal;
}*/

article, aside, details, figcaption, figure, footer, header, main, menu, nav, section {
  display: block;
  margin: 0;
}
h1, h2, h3, h4, h5, h6 {
  /*font-size: 100%;
  font-weight: normal;*/
}

[role="button"] {
  cursor: pointer;
}

html, body {
  width: 100%;
  height: 100%;
  line-height: 1.15;
  -ms-text-size-adjust: 100%;
  -webkit-text-size-adjust: 100%;
  font: 20px Arial,'Microsoft YaHei',SimHei;
  margin: 0;
  padding: 0;
  overflow: hidden;
}
body {
  padding: 54px 0 0 180px;
}
* {
  box-sizing: border-box;
}
.fontArial {
  font-family: Arial!important;
}
a {
  color: #4a4a4a;
  text-decoration: none !important;
  outline: none !important;
}

a:hover {
  text-decoration: none !important;
  color: #4a4a4a;
}

:after, :before, *::before, *::after {
  box-sizing: border-box;
}

/*box setting*/
.w {
  width: 100%;
}

.h {
  height: 100%;
}

.wh {
  width: 100%;
  height: 100%;
}

.db {
  display: block;
}

.dl {
  display: inline-block;
}

.hide {
  display: none;
}

.hidden {
  visibility: hidden;
}

/*clear and float*/
.clear::after, .row::after {
  content: "";
  display: table;
  clear: both;
}

.fl {
  float: left;
}

.fr {
  float: right;
}

/*cursor*/
.cursor {
  cursor: pointer;
}

/*font*/
.fs12 {
  font-size: 12px;
}

.fs14 {
  font-size: 14px;
}

.fs16 {
  font-size: 16px;
}

.fs18 {
  font-size: 18px;
}

.fs20 {
  font-size: 20px;
}

.fs24 {
  font-size: 24px;
}

/*position*/
.pr {
  position: relative;
}

.pa {
  position: absolute;
}

.pf {
  position: fixed;
}

/*text setting*/
.giveMeEllipsis {
  display: inline-block;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.t-left {
  text-align: left;
}

.t-center {
  text-align: center;
}

.t-right {
  text-align: right;
}

.t-normal {
  font-weight: normal;
}

.t-weight {
  font-weight: 700;
}

.t-weightM {
  font-weight: 400;
}

.t-italic {
  font-style: italic;
}

.t-transformD {
  text-transform: uppercase;
}

.t-transformX {
  text-transform: lowercase;
}

.t-transformXD {
  text-transform: capitalize;
}

.row {
  margin-right: -15px;
  margin-left: -15px;
}

.col {
  float: left;
  min-height: 1px;
  padding: 0 15px;
  position: relative;
}

.col_1 {
  width: 8.333333%;
}

.col_2 {
  width: 16.666667%;
}

.col_3 {
  width: 25%;
}

.col_4 {
  width: 33.333333%;
}

.col_5 {
  width: 41.666667%;
}

.col_6 {
  width: 50%;
}

.col_7 {
  width: 58.333333%;
}

.col_8 {
  width: 66.666667%;
}

.col_9 {
  width: 75%;
}

.col_10 {
  width: 83.333333%;
}

.col_11 {
  width: 91.666667%;
}

.col_12 {
  width: 100%;
}

/*页面中公用部分的样式*/
.container {
  opacity: 0;
  overflow-y: auto;
  background-color: #f6f6f6;
  padding: 0 32px 12px;
  -webkit-transition: all 0.3s;
  -moz-transition: all 0.3s;
  -ms-transition: all 0.3s;
  -o-transition: all 0.3s;
  transition: all 0.3s;
}
.header {
  height: 54px;
  background-color: #5581f8;
  top: 0;
  left: 0;
  z-index: 20;
  color: #fff;
}
.header .logo {
  padding-left: 100px;
  line-height: 54px;
  white-space: nowrap;
  background: url("../images/common/home_logo.png") no-repeat 28px center;
}
.header .avatarWrap {
  padding-right: 32px;
}
.header .avatarWrap .avatar {
  width: 30px;
  height: 30px;
  border-radius: 50%;
  overflow: hidden;
  margin-top: 12px;
}
.header .avatarWrap .name {
  line-height: 54px;
  white-space: nowrap;
  padding: 0 28px 0 14px;
}
.header .avatarWrap i {
  width: 10px;
  height: 4px;
  background: url("../images/common/aDown.png") no-repeat;
  top: 25px;
  right: 32px;
  -webkit-transform: rotate(0deg);
  -moz-transform: rotate(0deg);
  -ms-transform: rotate(0deg);
  -o-transform: rotate(0deg);
  transform: rotate(0deg);
  -webkit-transition: all 0.3s;
  -moz-transition: all 0.3s;
  -ms-transition: all 0.3s;
  -o-transition: all 0.3s;
  transition: all 0.3s;
}
.header .avatarWrap:hover i {
  -webkit-transform: rotate(-180deg);
  -moz-transform: rotate(-180deg);
  -ms-transform: rotate(-180deg);
  -o-transform: rotate(-180deg);
  transform: rotate(-180deg);
}
.header .avatarWrap ul {
  top: 54px;
  left: 0;
  z-index: 100;
  padding: 8px 0;
  -webkit-animation: avatar 0.3s forwards;
  -o-animation: avatar 0.3s forwards;
  animation: avatar 0.3s forwards;
}
.header .avatarWrap ul li {
  width: 124px;
  height: 48px;
  line-height: 48px;
  border-radius: 6px;
  padding-left: 46px;
  font-size: 14px;
  color: #333;
  background: #fff url("../images/common/loginOut.png") no-repeat 20px center;
  box-shadow: 0 0 30px 10px rgba(85,129,248,.05);
}
.header .avatarWrap:hover ul {
  display: block;
}
@keyframes avatar {
  0% {
    opacity: 0;
    margin-top: -100px;
  }
  50% {
    opacity: 0;
    margin-top: -20px;
  }
  100% {
    opacity: 1;
    margin-top: 0;
  }
}
